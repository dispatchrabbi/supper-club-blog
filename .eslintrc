{
  "extends": "airbnb-base",
  "env": {
    "node": true,
  },
  "rules": {
    "comma-dangle": ["error", {
        "arrays": "only-multiline",
        "objects": "only-multiline",
        "imports": "only-multiline",
        "exports": "only-multiline",
        "functions": "never",
    }],
    "import/first": ["error", ""],
    "max-len": ["warn", 120],
    "import/newline-after-import": "off",
    "no-case-declarations": "off",
    "object-shorthand": "off",
    "prefer-template": "off",
  },
}
